set -e
# Automatically added by dh_installinit/13.11.4
if [ "$1" = "remove" ] && [ -x "/etc/init.d/hwclock.sh" ] ; then
	chmod -x "/etc/init.d/hwclock.sh" >/dev/null || true
fi
if [ -z "${DPKG_ROOT:-}" ] && [ "$1" = "purge" ] ; then
	update-rc.d hwclock.sh remove >/dev/null
