let s:cpo_save = &cpo
set cpo&vim

CompilerSet makeprg=dart
CompilerSet errorformat=%E%f:%l:%c:\ Error:\ %m,
		       \%CTry\ %.%#,
		       \%Z\ %#^%\\+,
		       \%C%.%#,
		       \%-G%.%#

