  command -nargs=* CompilerSet setlocal <args>
endif

let s:cpo_save = &cpo
set cpo&vim

CompilerSet makeprg=dart2js
CompilerSet errorformat=%E%f:%l:%c:,
		       \%-GError:\ Compilation\ failed.,
		       \%CError:\ %m,
		       \%Z\ %#^%\\+,
		       \%C%.%#,
		       \%trror:\ %m,
		       \%-G%.%#

let &cpo = s:cpo_save
unlet s:cpo_save
